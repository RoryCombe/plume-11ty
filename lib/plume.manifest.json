  "complexTypes": {
    "NavLink": {
      "description": "Navigation link object",
      "properties": [
        {
          "name": "label",
          "type": "string",
          "required": true,
          "description": "Link text"
        },
        {
          "name": "url",
          "type": "string",
          "required": true,
          "description": "Link URL"
        }
      ]
    },
    "PricingPlan": {
      "description": "Pricing plan object",
      "properties": [
        {
          "name": "name",
          "type": "string",
          "required": true,
          "description": "Plan name"
        },
        {
          "name": "price",
          "type": "string",
          "required": true,
          "description": "Plan price (as string, e.g., '29')"
        },
        {
          "name": "period",
          "type": "string",
          "required": false,
          "default": "/month",
          "description": "Billing period (e.g., '/month', '/year')"
        },
        {
          "name": "features",
          "type": "array",
          "required": false,
          "description": "Array of feature strings"
        },
        {
          "name": "buttonText",
          "type": "string",
          "required": false,
          "default": "Get Started",
          "description": "Text for the plan button"
        },
        {
          "name": "link",
          "type": "string",
          "required": false,
          "default": "#",
          "description": "URL for the plan button"
        },
        {
          "name": "featured",
          "type": "boolean",
          "required": false,
          "default": false,
          "description": "Whether this plan is featured (only for pricing-2 and pricing-3)"
        }
      ]
    },
    "Feature": {
      "description": "Feature object",
      "properties": [
        {
          "name": "icon",
          "type": "string",
          "required": false,
          "default": "âœ¨",
          "description": "Icon emoji or character"
        },
        {
          "name": "title",
          "type": "string",
          "required": true,
          "description": "Feature title"
        },
        {
          "name": "description",
          "type": "string",
          "required": true,
          "description": "Feature description"
        }
      ]
    }
  },
  "categories": [
    {
      "name": "hero",
      "description": "Hero sections for landing pages"
    },
    {
      "name": "nav",
      "description": "Navigation bars and menus"
    },
    {
      "name": "pricing",
      "description": "Pricing tables and plans"
    },
    {
      "name": "features",
      "description": "Feature lists and showcases"
    }
  ]
