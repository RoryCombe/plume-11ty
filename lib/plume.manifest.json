        },
        {
          "name": "subtitle",
          "type": "string",
          "required": false,
          "default": "",
          "description": "Subheading text displayed below the title"
        },
        {
          "name": "ctaText",
          "type": "string",
          "required": false,
          "default": "",
          "description": "Text for the call-to-action button"
        },
        {
          "name": "ctaLink",
